{
  "name": "dnd",
  "version": "1.0.0",
  "description": "A DnD personal project",
  "homepage": "https://github.com/nathan-zaloum/dnd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathan-zaloum/dnd.git"
  },
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nathan-zaloum/dnd/issues"
  },
  "scripts": {
    "client": "webpack serve --config ./client/webpack.config.js --hot",
    "dev": "run-p dev:client dev:server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon server",
    "start": "node server",
    "build": "npm run webpack",
    "server": "node server",
    "test": "jest",
    "test:watch": "jest --watchAll --verbose",
    "webpack": "webpack --config ./client/webpack.config.js",
    "lint": "eslint --ext .js,.jsx .",
    "knex": "knex --knexfile ./server/db/knexfile.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "camelcase-keys": "^6.2.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.95.4",
    "react-router-dom": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.1",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.0",
    "@babel/plugin-transform-runtime": "7.13.1",
    "@babel/preset-env": "7.13.0",
    "@babel/preset-react": "7.12.13",
    "@testing-library/react": "11.2.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.1",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.4.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "sqlite3": "^5.0.2",
    "superagent": "6.1.0",
    "supertest": "^6.1.3",
    "webpack": "5.24.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
